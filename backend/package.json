        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "email-validator": "^2.0.4",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "jsonwebtoken": "^8.5.1",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "mongodb": "^3.3.3",
        "mongoose": "^6.0.14",
        "morgan": "^1.9.1",
        "relaxed-json": "^1.0.3",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.0.0-pre.43",
